# AppDistillery Platform - Environment Variables
# --------------------------------------------------------
# 1. Copy this file to apps/web/.env.local
# 2. Fill in the real values (do NOT commit .env.local)
# --------------------------------------------------------

# Supabase (new API key format, not legacy anon/service_role)
NEXT_PUBLIC_SUPABASE_URL=https://xxx.supabase.co
NEXT_PUBLIC_SUPABASE_PUBLISHABLE_KEY=sb_publishable_xxx
SUPABASE_SECRET_KEY=sb_secret_xxx

# AI Providers
ANTHROPIC_API_KEY=
OPENAI_API_KEY=
GOOGLE_GENERATIVE_AI_API_KEY=

# App
NEXT_PUBLIC_APP_URL=http://localhost:3000

# Sentry Error Monitoring (optional, production only)
NEXT_PUBLIC_SENTRY_DSN=
SENTRY_DSN=
SENTRY_ORG=
SENTRY_PROJECT=
SENTRY_AUTH_TOKEN=
